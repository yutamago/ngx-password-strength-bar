{
  "name": "ngx-password-strength-bar",
  "description": "Angular Password Strength Bar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yutamago/ngx-password-strength-bar.git"
  },
  "author": "Yutamago",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yutamago/ngx-password-strength-bar/issues"
  },
  "homepage": "https://github.com/yutamago/ngx-password-strength-bar#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-password-strength-bar-app",
    "build:dev": "ng build ngx-password-strength-bar --configuration development --watch",
    "build:prod": "ng build ngx-password-strength-bar --configuration production",

    "test:lib": "ng test ngx-password-strength-bar",
    "test:app": "ng test ngx-password-strength-bar-app",

    "test": "npm run test:ci:app && npm run test:ci:lib",
    "test:ci": "npm run test:app --watch=false && npm run test:lib --watch=false",
    "publish": "cd dist/ngx-password-strength-bar && npm publish"
  },
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
